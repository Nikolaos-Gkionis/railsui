                    <li>
                      <%= link_to "Add booking", "#", class: "md:py-1 px-4 hover:text-primary-500 focus:text-primary-600 w-full group text-zinc-50 block", data: { controller: "prevent", action: "click->prevent#prevent" } %>
                    </li>

                    <li class="border-b border-zinc-600/80 my-1.5"></li>
                    <li>
                      <%= link_to "Account", "#", class: "py-1.5 px-4 hover:text-primary-500 focus:text-primary-600 w-full group text-zinc-50 font-light text-sm block text-left", data: { controller: "prevent", action: "click->prevent#prevent" } %>
                    </li>

                    <li>
                      <%= link_to "Sign out", "#", class: "py-1.5 px-4 hover:text-primary-500 focus:text-primary-600 w-full group text-zinc-50 font-light text-sm block text-left", data: { controller: "prevent", action: "click->prevent#prevent" } %>
                    </li>
                  </ul>
                </div>
              </li>
            </ul>
          </nav>
        </div>
      </div>
    </div>
  </div>
</header>
<% end %>


