<div class="my-16">

<%= render "railsui/shared/sticky_subhead", title: "Breadcrumbs", subtitle: "Dark contained" %>

<%= render preview("dark_zinc") do %>
<nav aria-label="breadcrumb" class="my-6 font-medium flex text-zinc-200 text-sm bg-zinc-800 rounded items-center border-zinc-600/80 border overflow-hidden sm:py-0 py-4">
  <ol class="flex items-center justify-star space-x-6 px-4 list-none">
    <li class="sm:h-12 flex items-center justify-center">
      <a href="#" class="hover:underline text-primary-500 hover:text-primary-600 inline-block pl-2 group" data-controller="prevent" data-action="click->prevent#prevent">
        <%= icon "home", classes: "w-5 h-5 text-zinc-300 group-hover:text-primary-500" %>
        <span class="sr-only">Home</span>
      </a>
    </li>
    <li class="sm:h-12 flex items-center space-x-2 justify-center">
      <a href="#" class="hover:underline text-primary-500 hover:text-primary-600" data-controller="prevent" data-action="click->prevent#prevent">Library</a>
    </li>
    <li class="sm:h-12 flex items-center space-x-2 justify-center">
      <a href="#" aria-current="page" class="text-zinc-100" data-controller="prevent" data-action="click->prevent#prevent">Data</a>
    </li>
  </ol>
</nav>
<% end %>

