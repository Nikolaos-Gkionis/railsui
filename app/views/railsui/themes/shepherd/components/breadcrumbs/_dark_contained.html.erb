        <span class="sr-only">Home</span>
      <%% end %>
    </li>
    <li class="sm:h-12 flex items-center space-x-2 justify-center">
      <%%= link_to "Library", "#", class: "hover:underline text-primary-500 hover:text-primary-600"%>
    </li>
    <li class="sm:h-12 flex items-center space-x-2 justify-center">
      <%%= link_to "Data", "#", class: "text-zinc-100", data: { "aria-current": "page" } %>
