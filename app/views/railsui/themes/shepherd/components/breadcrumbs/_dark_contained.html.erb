<% content_for :haml, flush: true do %>
%nav.my-6.font-medium.flex.text-zinc-200.text-sm.bg-zinc-800.rounded.items-center.border.overflow-hidden.sm:py-0.py-4(aria-label="breadcrumb" class="border-zinc-600/80")
  %ol.flex.items-center.justify-star.space-x-6.px-4.list-none
    %li.sm:h-12.flex.items-center.justify-center
      = link_to "#", class: "hover:underline text-primary-500 hover:text-primary-600 inline-block pl-2 group" do
        = icon "home", classes: "w-5 h-5 text-zinc-300 group-hover:text-primary-500"
        %span.sr-only Home
